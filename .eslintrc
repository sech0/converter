{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "require-jsdoc": "off",
    "linebreak-style": ["error", "windows"],
    "semi": [2, "never"],
    "max-len": [2, { "code": 120, "tabWidth": 2, "ignoreUrls": true }]
  },
  "extends": ["eslint:recommended", "google"]
}
